{
    "$schema": "https://docs.renovatebot.com/renovate-schema.json",
    "extends": [
        "config:base",
        "group:allNonMajor"
    ],
    "labels": [
        "dependencies"
    ],
    "packageRules": [
        {
            "matchManagers": [
                "dockerfile"
            ],
            "labels": [
                "dependencies::docker"
            ],
            "groupName": "Docker"
        }
    ]
}
